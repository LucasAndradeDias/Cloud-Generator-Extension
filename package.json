{
  "name": "cloud-generator",
  "displayName": "CloudGenerator",
  "description": "A tool to help to build Cloud Computing serveless projects",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:CloudGenerator.createAppEnginePython",


    "onCommand:CloudGenerator.createHttpPythonFunction",
    "onCommand:CloudGenerator.createHttpNodeProject",
    
    "onCommand:CloudGenerator.createEventPythonFunction",
    "onCommand:CloudGenerator.createEventNodeProject",
    "onCommand:CloudGenerator.createEventGoProject",
    "onCommand:CloudGenerator.createEventJavaProject"

  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      

      {"command": "CloudGenerator.createAppEnginePython","category": "Cloud-generator","title": "Create python app engine project"},



      {"command": "CloudGenerator.createHttpPythonFunction","category": "Cloud-function-generator","title": "Create python http cloud function project"},
      {"command": "CloudGenerator.createHttpNodeProject","category": "Cloud-function-generator","title": "Create node http cloud function project"},
      
      {"command": "CloudGenerator.createEventNodeProject","category": "Cloud-function-generator","title": "Create node event cloud function project"},      
      {"command": "CloudGenerator.createEventPythonFunction","category": "Cloud-function-generator","title": "Create python event cloud function project"},
      {"command": "CloudGenerator.createEventGoProject","category": "Cloud-function-generator","title": "Create golang event cloud function project"},
      {"command": "CloudGenerator.createEventJavaProject","category": "Cloud-function-generator","title": "Create java event cloud function project"}
  
    
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
