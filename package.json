{
  "name": "cloud-functions-generator",
  "displayName": "cloudFunctionsGenerator",
  "description": "Generate a pre build cloud functions project ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cloudFunctionsGenerator.createPythonFunction",
    "onCommand:cloudFunctionsGenerator.createNodeProject",
    "onCommand:cloudFunctionsGenerator.createGoProject",
    "onCommand:cloudFunctionsGenerator.createJavaProject"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {"command": "cloudFunctionsGenerator.createPythonFunction","category": "Cloud-function-generator","title": "Create python cloud function project"},
      {"command": "cloudFunctionsGenerator.createNodeProject","category": "Cloud-function-generator","title": "Create node cloud function project"},
      {"command": "cloudFunctionsGenerator.createGoProject","category": "Cloud-function-generator","title": "Create go cloud function project"},
      {"command": "cloudFunctionsGenerator.createJavaProject","category": "Cloud-function-generator","title": "Create java cloud function project"}
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
